{
  "name": "redux-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate for React+Redux applications",
  "main": "index.js",
  "repository": "git@github.com:ebfjunior/redux-boilerplate.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:development": "webpack --config webpack.config.js",
    "build:production": "cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "jest": {
    "setupFiles": [
      "./__test__/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "author": "Edson JÃºnior",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bulma": "^0.7.1",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "dedupe-plugin": "^1.0.1",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.1.0",
    "jsdom": "^11.11.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.0.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.0.4",
    "redux-thunk": "^2.3.0"
  }
}
